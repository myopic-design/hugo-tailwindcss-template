{
  "private": true,
  "name": "hugo-tailwindcss-template",
  "version": "1.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/myopic-design/hugo-tailwindcss-template.git"
  },
  "scripts": {
    "clean": "rm -rf .lighthouseci e2e-report e2e-results public resources/_gen .hugo_build.lock hugo_stats.json",
    "dev": "hugo server --buildDrafts --buildExpired --buildFuture",
    "build": "hugo --cleanDestinationDir --minify",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@playwright/test": "1.35.0",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.1",
    "husky": "8.0.3",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-go-template": "0.0.13",
    "prettier-plugin-tailwindcss": "0.3.0",
    "tailwindcss": "3.3.2"
  },
  "packageManager": "pnpm@8.6.1"
}
